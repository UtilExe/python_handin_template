    "import re\n",
    "txt = \"\"\"Store Pizza stykker\n",
    "Lille blå Pizzaria\n",
    "Byens bedste Pizzeria\n",
    "Pizzaria la costa\n",
    "Testing hej\n",
    "\"\"\"\n",
    "\n",
    "pattern = re.compile(r'([\\w ]+)*(Pizza|Pizzeria|Pizzaria)([\\w ]+)*')\n",
    "\n",
    "for line in txt.split('\\n'):\n",
    "    print(pattern.match(line))"
   ]
